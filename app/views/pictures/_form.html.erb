<%= form_for(@insta) do |f| %>
 <% if @insta.errors.any? %>
 <div id="error_explanation">
 <h2><%=@insta.errors.count %>件のエラーがあります。</h2>

 <ul>
 <% @insta.errors.full_messages.each do |msg| %>
 <li><%= msg %></li>
 <% end %>
 </ul>
 </div>
 <% end %>
<%= f.label :gazo %>
<%= f.file_field :gazo %>
<%= f.hidden_field :gazo_cache %>
<br>
 <%= f.label :title %>
 <%= f.text_field :title,class:'form-control' %>
 <br>
 <%= f.label :content %>
 <%= f.text_field :content,class:'form-control' %>
 <br>
 <%= f.submit'投稿',class:'btn btn-primary btn-block' %>
 <% end %>

 <%=link_to "投稿一覧画面にもどる",pictures_path %>
