<div class="container">
  <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
    <h3>以下の内容で登録する</h3>
    <%= form_for(@insta) do |f| %>
    <%= f.hidden_field :title %>
    <%= f.hidden_field :content %>

    <p>タイトル:<%= @insta.title %></p>
    <p>本文:<%= @insta.content %></p>
    <%= f.submit "登録",class:'btn btn-primary btn-block' %>
    <% end %>

    <%= form_for(@insta,url: new_picture_path,method:'get') do |f| %>
    <%= f.hidden_field :title %>
    <%= f.hidden_field :content %>
    <%= f.submit"もどる",name: 'back',class:'btn btn-primary btn-block' %>
    <% end %>
  </div>
</div>
